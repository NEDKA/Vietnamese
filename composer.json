{
	"$schema": "https://getcomposer.org/schema.json",
	"name": "nedka/vietnamese",
	"type": "library",
	"description": "The Vietnamese library for PHP.",
	"keywords": [
		"vietnamese",
		"php"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Khoa Nguyen",
			"email": "hi@nedka.com"
		}
	],
	"support": {
		"email": "hi@nedka.com"
	},
	"require": {
		"php": ">=8.0",
		"ext-mbstring": "*"
	},
	"autoload": {
		"psr-4": {
			"NEDKA\\Vietnamese\\": "src"
		}
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
